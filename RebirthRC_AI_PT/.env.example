# Rebirth RC AI Offensive Security Tool - Environment Configuration
# Copy this file to .env and fill in your actual values

# ============================================
# API Keys (Required)
# ============================================
# OpenAI API Key (for GPT and Codex models)
OPENAI_API_KEY=your_openai_api_key_here

# 5 Hihg API Key (Optional - will use OpenAI as fallback if not set)
HIHG_API_KEY=your_hihg_api_key_here

# Custom API Base URLs (Optional - only if using custom endpoints)
OPENAI_BASE_URL=https://api.openai.com/v1
HIHG_BASE_URL=

# ============================================
# Redis Configuration
# ============================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# ============================================
# Game Configuration
# ============================================
# Path to the game executable
# Option 1: Use RebirthClient.exe in project directory (default)
# GAME_PATH=./RebirthClient.exe

# Option 2: Use alternative path from RebithPatcher.lnk shortcut
# GAME_PATH=C:\RebirthRC\content\RebirthRC.exe

# Option 3: Custom path
# GAME_PATH=C:\Program Files\RebirthRC\RebirthRC.exe

# Alternative game path (from RebithPatcher.lnk)
GAME_ALTERNATIVE_PATH=C:\RebirthRC\content\RebirthRC.exe

# Path to game logs directory
GAME_LOG_PATH=C:\Users\%USERNAME%\Documents\RebirthRC\logs\

# Game process name (must match actual running process)
GAME_PROCESS_NAME=RebirthClient.exe

# Game server information (if known)
GAME_SERVER_IP=
GAME_SERVER_PORT=7777

# Network scan range (for reconnaissance)
NMAP_SCAN_RANGE=192.168.1.0/24

# Proxy server (for traffic interception, e.g., Burp Suite)
PROXY_SERVER=http://127.0.0.1:8080

# ============================================
# AI Model Configuration
# ============================================
# Planner Agent (Strategic Brain - GPT)
PLANNER_MODEL=gpt-4-turbo-preview
PLANNER_TEMPERATURE=0.7

# Executor Agent (Code Generator - Codex)
EXECUTOR_MODEL=gpt-3.5-turbo-instruct
EXECUTOR_TEMPERATURE=0.1

# Observer Agent (Real-time Monitor - 5 Hihg)
OBSERVER_MODEL=gpt-3.5-turbo
OBSERVER_TEMPERATURE=0.0

# Reverse Engineer Agent (Code Analyst - Codex)
REVERSE_ENGINEER_MODEL=gpt-3.5-turbo-instruct
REVERSE_ENGINEER_TEMPERATURE=0.3

# Fuzzer Agent (Payload Generator - 5 Hihg)
FUZZER_MODEL=gpt-3.5-turbo
FUZZER_TEMPERATURE=0.9

# ============================================
# System Configuration
# ============================================
# Log directory
LOG_DIR=logs

# Maximum log entries to keep
MAX_LOG_ENTRIES=1000

# Agent polling interval (seconds)
AGENT_POLL_INTERVAL=5.0

